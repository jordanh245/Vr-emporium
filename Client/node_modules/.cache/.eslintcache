 = {\n\t\t\tfirstname,\n\t\t\tlastname,\n\t\t\temail,\n\t\t\tpassword\n\t\t};\n\n\t\tconst response = await fetch('http://localhost:3001/register',\n\t\t{\n\t\t\tmethod: 'POST',\n\t\t\theaders: {\n\t\t\t\t'Content-type': 'application/json',\n\t\t\t},\n\t\t\tbody: JSON.stringify(body)\n\t\t\n\t\t\n\t\t}\n\t\t);\n\n\t} catch (err) {\n\t\tconsole.error(err.message);\n\t}\n\t\n};\n\t\n\t\n\t\n\t\n\treturn (\n\t\t<div>\n\t\t\t<form onSubmit={submitForm}>\n\t\t<input type=\"text\" placeholder=\"firstname\" name=\"firstname\"value={firstname}  onChange={(e)=> submit(e)}/>\n\t\t<input type=\"text\" placeholder=\"lastname\" name=\"lastname\" value={lastname} onChange={(e)=> submit(e)}/>\n\t\t<input type=\"email\" placeholder=\"email\" name=\"email\" value={email}  onChange={(e)=> submit(e)}/>\n\t\t<input type=\"password\" placeholder=\"password\"  name=\"password\" value={password}  onChange={(e)=> submit(e)} />\n\t\t<button >SUBMIT</button>\n\t\t\t</form>\n\t\t</div>\n\t)\n\n};\n\n","/Users/jordanharris/Desktop/Vr-emporium/Client/src/components/Product.js",["148","149","150"],"/Users/jordanharris/Desktop/Vr-emporium/Client/src/components/NavbarInfo.js",["151"],"\n\n// BASE LAYOUT!!!!!!!!\n\nimport React from \"react\";\nimport { connect } from \"react-redux\";\nimport NavLinks from \"./NavLinks\";\n\n\nfunction NavbarInfo(props) {\n  return (\n    <div>\n      \n\t  <NavLinks/>\n\t  \n\t  {props.children}\n    </div>\n  );\n}\n\n\nexport default NavbarInfo;\n","/Users/jordanharris/Desktop/Vr-emporium/Client/src/components/Home.js",["152","153"],"import React from 'react'\nimport {ButtonStyle, Image, ImageDiv, Center, InfoDiv, WhoH1, WhoP, FooterDiv, Social, SocialP} from \"../styled-components/HomeStyle\"\nimport {Link } from 'react-router-dom'\nimport social from \"../assets/twitter (2).png\"\n\nimport social3 from \"../assets/instagram.png\"\nimport background1 from \"../assets/background1.png\"\n\n\n\nexport default function home() {\n\t\n\t\n\t\n\t\n\t\n\treturn (\n\t\t